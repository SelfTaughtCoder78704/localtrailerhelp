<% include partials/header %>

<h1 class="text-center">Add Your Trailer</h1>
<div id="addForm"class="container text-center">
<form class="form-group" action="/trailers" method="POST">
    <input class="form-control" type="text" name="kind" placeholder="Trailer Type">
    <input type="text" class="form-control" name="img" placeholder="Image Url">
    <textarea class="form-control" name="desc" id="desc" cols="30" rows="10"></textarea>
    <button class="btn btn-primary searchBtn">Submit</button>
</form>

</div>
<a href="/trailers" class="btn btn-primary searchBtn" >Back</a>
